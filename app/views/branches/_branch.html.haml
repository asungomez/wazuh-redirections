.list-group-item.d-flex.justify-content-between.align-items-center
  = branch.version
  %div
    = link_to 'Delete', branch, method: :delete, class: 'btn btn-danger'
    = link_to 'Edit', edit_branch_path(branch), class: 'btn btn-warning ml-2'
    = link_to 'Show pages', branch, class: 'btn btn-primary ml-4'